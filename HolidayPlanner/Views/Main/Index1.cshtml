        var params = [];
        for (var i in filter) {
            if (filter[i] instanceof Array && filter[i].length > 0) {

                params.push(i + "=" + filter[i].join(","));
            }
            else if (!(filter[i] instanceof Array) && filter[i] != undefined && filter[i] != 0) {

                params.push(i + "=" + filter[i]);
            }

        }
        return encodeURI(params.join("&"));
