            var v = parts[1];
            var add = true;
            if (link.hasClass("ready"))
                add = false;
            link.toggleClass("ready");

            switch (k) {
                case "locationtype":
                    if (add)
                        inputparams.locationtype = v;
                    else {
                        inputparams.locationtype = "";
                    }
                    inputparams.minbudget = 0;
                    inputparams.maxbudget = 0;
                    break;
                case "district":
