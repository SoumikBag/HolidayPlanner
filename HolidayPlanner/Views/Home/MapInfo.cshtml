@model HolidayPlanner.Models.Location
@{
    Layout = null;
}
<style>

    #frame {
        border: 1px solid black;
        overflow: hidden;
        -ms-zoom: 0.75;
        -moz-transform: scale(0.75);
        -moz-transform-origin: 0 0;
        -o-transform: scale(0.75);
        -o-transform-origin: 0 0;
        -webkit-transform-origin: 0 0;
    }
</style>


<center><iframe id="frame" width="900" height="600" frameborder="1"  scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?q=+@Model.Latitude+,+@Model.Longitude+&hl=es;z=14&amp;output=embed"></iframe></center>