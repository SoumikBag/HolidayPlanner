        var initpagination = function (url) {
            $('#resultoptionlist').data("currentpage", 2);
            var footerheight = $("#footer").outerHeight();
            $('#resultoptionlist').scrollPagination({
                'contentPage': url,
                'contentData': { 'page': 2 },
                'scrollTarget': $(window),
                'heightOffset': footerheight + 10,
                'beforeLoad': function () {
                    $("#resultlistloading").fadeIn();
                },
                'afterLoad': function (elementsLoaded) {
                    $("#resultlistloading").fadeOut();
                    var elements = elementsLoaded.filter(".resultoption");

                    if (elements.size() == 0) {
                        $('#resultoptionlist').find(".nooptionsfound").remove();
                        $('#resultoptionlist').append("<p style='text-align:center;padding:0.5em 0;font-size:2em;color:black;background:white;border:1px solid #bbb;'>End of Results</p>");
                        $('#resultoptionlist').stopScrollPagination();
                        return false;
                    }
                    var currentpage = $('#resultoptionlist').data("currentpage");
                    currentpage++;
                    $('#resultoptionlist').data("currentpage", currentpage);
                    $('#resultoptionlist').scrollPagination.defaults.contentData = { 'page': currentpage };
                    $("#resultoptionlist").find(".wrapperin").dotdotdot();
                    $(elements).fadeInWithDelay();
                    adjustimages();
                }
            });

            $.fn.fadeInWithDelay = function () {
                var delay = 0;
                return this.each(function () {
                    $(this).delay(delay).animate({ opacity: 1 }, 200);
                    delay += 100;
                });
            };
        };

        var adjustimages = function () {
            $("#resultoptionlist .resultoption .detailsection").each(function () {
                var height = $(this).height();
                var imagesectionheight = $(this).prev(".imagesection").height();
                if (imagesectionheight < height) {
                    $(this).prev(".imagesection").css("height", height);
                }
            });
        };
        var parsehash = function (hash) {
            var elems = hash.split("&");

            for (var i in elems) {

                var t = inputparams[elems[i].split("=")[0]];
                if (t !== undefined) {
                    if (t instanceof Array) {
                        var array = elems[i].split("=")[1].split(",");
                        inputparams[elems[i].split("=")[0]] = array;
                    }
                    else {
                        inputparams[elems[i].split("=")[0]] = elems[i].split("=")[1];
                    }
                }
            }

        };

